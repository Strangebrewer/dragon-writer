{
  "name": "writing_tool",
  "version": "1.0.0",
  "description": "",
  "engines": {
    "node": "8.10.0"
  },
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/dbSeed.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^3.5.0",
    "nodemon": "^1.17.5"
  },
  "dependencies": {
    "axios": "^0.16.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "cloudinary": "^1.13.2",
    "cloudinary-core": "^2.5.0",
    "date-fns": "^1.29.0",
    "dotenv": "^6.2.0",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "immutable": "^4.0.0-rc.12",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.0.17",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "react": "^16.6.1",
    "react-adopt": "^0.6.0",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-dom": "^16.6.1",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-sortable-hoc": "^0.8.4",
    "slate": "^0.44.4",
    "slate-react": "^0.21.10"
  }
}
